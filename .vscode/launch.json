{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "c Build&Debug",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/build/poc",
        "args": [],
        "stopAtEntry": true,
        "cwd": "${workspaceFolder}/build",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "miDebuggerPath": "/usr/bin/gdb",
        "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "build"
      }
    ]
  }